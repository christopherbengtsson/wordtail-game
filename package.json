{
  "name": "word-tail",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "typecheck": "tsc --emitDeclarationOnly false --noEmit",
    "generate-types": "tsc",
    "lint": "eslint src/ && npm run typecheck",
    "lint:fix": "eslint --fix src/",
    "preview": "vite preview",
    "supabase:start": "npx supabase start",
    "supabase:stop": "npx supabase stop",
    "db:reset": "npx supabase db reset",
    "db:add:migration": "npx supabase migration up",
    "e2e:integration": "E2E_CI=1 npm run e2e:run",
    "e2e:run": "npx playwright test --config e2e/playwright.config.ts",
    "e2e:debug": "npx playwright test --config e2e/playwright.config.ts --debug",
    "e2e:ui": "npx playwright test --config e2e/playwright.config.ts --ui"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.36.0",
    "@tanstack/react-query": "^4.35.3",
    "@types/react-modal": "^3.16.1",
    "date-fns": "^2.30.0",
    "formik": "^2.4.5",
    "lodash.debounce": "^4.0.8",
    "mobx": "^6.10.2",
    "mobx-react": "^9.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-modal": "^3.16.1",
    "react-router-dom": "^6.16.0",
    "react-select": "^5.7.7",
    "react95": "^4.0.0",
    "styled-components": "^6.1.0",
    "yup": "^1.3.2"
  },
  "devDependencies": {
    "@playwright/test": "^1.38.1",
    "@tanstack/react-query-devtools": "^4.35.7",
    "@testing-library/dom": "^9.3.3",
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/ip": "^1.1.1",
    "@types/lodash.debounce": "^4.0.7",
    "@types/node": "^20.8.3",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "axe-playwright": "^1.2.3",
    "babel-plugin-styled-components": "^2.1.4",
    "eslint": "^8.45.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "ip": "^1.1.8",
    "supabase": "^1.99.5",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
  }
}
